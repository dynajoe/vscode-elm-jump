{
  "name": "vscode-elm-jump",
  "displayName": "vscode-elm-jump",
  "description": "",
  "version": "0.0.1",
  "publisher": "joeandaverde",
  "engines": {
    "vscode": "^1.5.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:elm"
  ],
  "main": "./out/src/extension",
  "scripts": {
    "vscode:prepublish": "elm-make src/elm/Main.elm --output out/src/elm.js && tsc -p ./",
    "compile": "elm-make src/elm/Main.elm --output out/src/elm.js && tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.64",
    "@types/mocha": "^2.2.32",
    "@types/node": "^6.0.40",
    "mocha": "^2.3.3",
    "typescript": "^2.0.3",
    "vscode": "^1.0.0"
  },
  "dependencies": {
    "lodash": "^4.17.4"
  }
}
